{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api-gateway/trippin",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@domjtalbot/nx-plugin-graphql-mesh:build-gateway",
      "outputs": ["{options.outputPath}"],
      "options": {
        "dir": "apps/api-gateway/trippin",
        "outputPath": "dist/apps/api-gateway/trippin"
      },
      "configurations": {
        "e2e": {
          "debug": false
        }
      }
    },
    "dev": {
      "executor": "@domjtalbot/nx-plugin-graphql-mesh:dev",
      "options": {
        "dir": "apps/api-gateway/trippin",
        "port": 4500
      },
      "configurations": {
        "e2e": {
          "debug": false,
          "port": 5001
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/api-gateway/trippin/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/api-gateway/trippin"],
      "options": {
        "jestConfig": "apps/api-gateway/trippin/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:app"]
}
