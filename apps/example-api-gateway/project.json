{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/example-api-gateway",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@domjtalbot/nx-plugin-graphql-mesh:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "dir": "apps/example-api-gateway",
        "outputPath": "dist/apps/example-api-gateway"
      },
      "configurations": {
        "e2e": {
          "debug": false
        }
      }
    },
    "dev": {
      "executor": "@domjtalbot/nx-plugin-graphql-mesh:dev",
      "options": {
        "dir": "apps/example-api-gateway",
        "port": 4500
      },
      "configurations": {
        "e2e": {
          "debug": false,
          "port": 4600
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/example-api-gateway/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/example-api-gateway"],
      "options": {
        "jestConfig": "apps/example-api-gateway/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:app"]
}
