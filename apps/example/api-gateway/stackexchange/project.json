{
  "name": "example-api-gateway-stackexchange",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/example/api-gateway/stackexchange/",
  "targets": {
    "build": {
      "executor": "nx-mesh:build-gateway",
      "outputs": [
        "apps/example/api-gateway/stackexchange/.mesh",
        "{options.outputPath}"
      ],
      "options": {
        "dir": "apps/example/api-gateway/stackexchange",
        "main": "apps/example/api-gateway/stackexchange/src/index.ts",
        "outputPath": "dist/apps/example/api-gateway/stackexchange",
        "tsConfig": "apps/example/api-gateway/stackexchange/tsconfig.lib.json"
      }
    },
    "serve": {
      "executor": "nx-mesh:serve",
      "options": {
        "dev": true,
        "dir": "apps/example/api-gateway/stackexchange"
      }
    },
    "validate": {
      "executor": "nx-mesh:validate",
      "options": {
        "dir": "apps/example/api-gateway/stackexchange"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/example/api-gateway/stackexchange/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/example/api-gateway/stackexchange/**/*.ts"]
      }
    }
  },
  "tags": ["type:api-gateway", "type:example"]
}
