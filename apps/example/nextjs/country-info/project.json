{
  "name": "example-nextjs-country-info",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/example/nextjs/country-info",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/example/nextjs/country-info",
        "outputPath": "dist/apps/example/nextjs/country-info"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/example/nextjs/country-info"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "example-nextjs-country-info:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "example-nextjs-country-info:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "example-nextjs-country-info:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "example-nextjs-country-info:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/example/nextjs/country-info/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/example/nextjs/country-info/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": ["type:example", "type:nextjs"]
}
