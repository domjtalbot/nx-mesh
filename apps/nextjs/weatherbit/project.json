{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nextjs/weatherbit",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/nextjs/weatherbit",
        "outputPath": "dist/apps/nextjs/weatherbit"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "nextjs-weatherbit:build",
        "dev": true,
        "port": 4400
      },
      "configurations": {
        "development": {
          "buildTarget": "nextjs-weatherbit:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "nextjs-weatherbit:build:production",
          "dev": false
        }
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "nextjs-weatherbit:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/nextjs/weatherbit"],
      "options": {
        "jestConfig": "apps/nextjs/weatherbit/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/nextjs/weatherbit/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
