{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nx-plugin-graphql-mesh-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "defaultConfiguration": "dev",
      "options": {
        "cypressConfig": "apps/nx-plugin-graphql-mesh-e2e/cypress.json"
      },
      "configurations": {
        "build": {
          "devServerTarget": "example-nextjs-stackexchange:serve:production",
          "spec": "**/**build.spec**"
        },
        "dev": {
          "devServerTarget": "example-api-gateway:dev:e2e",
          "spec": "**/**dev.spec**"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/nx-plugin-graphql-mesh-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["example-api-gateway", "nx-plugin-graphql-mesh"]
}
