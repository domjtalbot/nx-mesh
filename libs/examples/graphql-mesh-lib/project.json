{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/examples/graphql-mesh-lib/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@domjtalbot/nx-plugin-graphql-mesh:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "dir": "libs/examples/graphql-mesh-lib",
        "outputPath": "dist/libs/examples/graphql-mesh-lib",
        "tsConfig": "libs/examples/graphql-mesh-lib/tsconfig.lib.json",
        "main": "libs/examples/graphql-mesh-lib/src/index.ts"
      },
      "configurations": {
        "e2e": {
          "debug": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/examples/graphql-mesh-lib/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/examples/graphql-mesh-lib"],
      "options": {
        "jestConfig": "libs/examples/graphql-mesh-lib/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
