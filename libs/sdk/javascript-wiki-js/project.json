{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/sdk/javascript-wiki-js/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@domjtalbot/nx-plugin-graphql-mesh:build",
      "outputs": ["libs/sdk/javascript-wiki-js/.mesh", "{options.outputPath}"],
      "options": {
        "dir": "libs/sdk/javascript-wiki-js",
        "main": "libs/sdk/javascript-wiki-js/src/index.ts",
        "outputPath": "dist/libs/sdk/javascript-wiki-js",
        "tsConfig": "libs/sdk/javascript-wiki-js/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/sdk/javascript-wiki-js/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/sdk/javascript-wiki-js"],
      "options": {
        "jestConfig": "libs/sdk/javascript-wiki-js/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve": {
      "executor": "@domjtalbot/nx-plugin-graphql-mesh:serve",
      "options": {
        "dev": true,
        "dir": "libs/sdk/javascript-wiki-js",
        "port": 4200
      }
    },
    "validate": {
      "executor": "@domjtalbot/nx-plugin-graphql-mesh:validate",
      "options": {
        "dir": "libs/sdk/javascript-wiki-js"
      }
    }
  },
  "tags": []
}
